<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".view.practice.Practice_Enrichment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/topPanel"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="543dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/practice1" />

            <ImageView
                android:id="@+id/mtMain1"
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/home" />
        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/topPanel"
            app:layout_constraintBottom_toBottomOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_ga"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="ga"
                        app:srcCompat="@drawable/ga_bt" />

                    <ImageView
                        android:id="@+id/Evowel_na"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="na"
                        app:srcCompat="@drawable/na_bt" />

                    <ImageView
                        android:id="@+id/Evowel_da"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="da"
                        app:srcCompat="@drawable/da_bt" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_gu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="gu"
                        app:srcCompat="@drawable/gu_bt" />

                    <ImageView
                        android:id="@+id/Evowel_nu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="nu"
                        app:srcCompat="@drawable/nu_bt" />

                    <ImageView
                        android:id="@+id/Evowel_du"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="du"
                        app:srcCompat="@drawable/du_bt" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_go"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="go"
                        app:srcCompat="@drawable/go_bt" />

                    <ImageView
                        android:id="@+id/Evowel_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="no"
                        app:srcCompat="@drawable/no_bt" />

                    <ImageView
                        android:id="@+id/Evowel_do"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="do"
                        app:srcCompat="@drawable/do_bt" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_gi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="gi"
                        app:srcCompat="@drawable/gi_bt" />

                    <ImageView
                        android:id="@+id/Evowel_ni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="ni"
                        app:srcCompat="@drawable/ni_bt" />

                    <ImageView
                        android:id="@+id/Evowel_di"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="di"
                        app:srcCompat="@drawable/di_bt" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_ge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="ge"
                        app:srcCompat="@drawable/ge_bt" />

                    <ImageView
                        android:id="@+id/Evowel_ne"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="ne"
                        app:srcCompat="@drawable/ne_bt" />

                    <ImageView
                        android:id="@+id/Evowel_de"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="de"
                        app:srcCompat="@drawable/de_bt" />

                </FrameLayout>


                <FrameLayout
                    android:layout_width="650dp"
                    android:layout_height="95dp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="30dp"
                    android:layout_marginTop="30dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Evowel_geu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="geu"
                        app:srcCompat="@drawable/geu_bt" />

                    <ImageView
                        android:id="@+id/Evowel_neu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="neu"
                        app:srcCompat="@drawable/neu_bt" />

                    <ImageView
                        android:id="@+id/Evowel_deu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="40dp"
                        android:adjustViewBounds="true"
                        android:scaleType="fitCenter"
                        android:tag="deu"
                        app:srcCompat="@drawable/deu_bt" />
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>